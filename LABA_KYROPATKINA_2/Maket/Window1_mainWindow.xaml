        <TextBlock HorizontalAlignment="Right"  Foreground="#7DAAC7" FontSize="20" Margin="0,12.417,0,-27.083" Grid.RowSpan="11" Width="74.857">
                    VIRTUAL
        </TextBlock>



        <TextBlock HorizontalAlignment="Left"  Foreground="#C5D8E7" FontSize="20" Margin="0,12.15,0,-30.927" Grid.RowSpan="11" Grid.Column="1" Width="69.943" >
                    HEALTH
        </TextBlock>










