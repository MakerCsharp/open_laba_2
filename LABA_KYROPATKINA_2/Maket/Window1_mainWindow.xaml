                    <ColumnDefinition Width="53*"/>
                    <ColumnDefinition Width="62*"/>
                    <ColumnDefinition Width="83*"/>
                </Grid.ColumnDefinitions>

                <Grid Grid.Row="0" Grid.Column="0">
                    <Button  HorizontalAlignment="Center"  VerticalAlignment="Bottom"  Width="50" Style="{DynamicResource ButtonStyle_1day}"  Height="20" Grid.Row="0" Grid.Column="0" />
                </Grid>

                <Grid Grid.Row="0" Grid.Column="1">
                    <Button  HorizontalAlignment="Center" VerticalAlignment="Bottom"   Width="50" Style="{DynamicResource ButtonStyle_7days}"  Height="20"   Grid.Row="0" Grid.Column="1"  />
                </Grid>

                <Grid Grid.Row="0" Grid.Column="2">
                    <Button  HorizontalAlignment="Center"  VerticalAlignment="Bottom"  Content="30 days"  Width="54" Style="{DynamicResource ButtonStyle_30days}"  Height="20" Grid.Row="0" Grid.Column="2"  />
                </Grid>

                <Grid Grid.Row="0" Grid.Column="3">
                    <Button  HorizontalAlignment="Center"  VerticalAlignment="Bottom"  Content="90 days"  Width="50" Style="{DynamicResource ButtonStyle_90days}"  Height="20"   Grid.Row="0" Grid.Column="3" />
                </Grid>

                <Grid Grid.Row="0" Grid.Column="4">
                    <Button  HorizontalAlignment="Center"  VerticalAlignment="Bottom"  Content="All"  Width="50" Style="{DynamicResource ButtonStyle_Alldays}"  Height="20"  Grid.Row="0" Grid.Column="4"  />
                </Grid>
               

                
               
                
                
