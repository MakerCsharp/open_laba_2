        <Button Content="Tasks" x:Name="Tasks" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" Width="52" Style="{DynamicResource ButtonStyle_Page_1_Tasks}" Height="20" Margin="15.554,19,22.355,11" Grid.Row="2"   />
