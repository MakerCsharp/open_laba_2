        <Rectangle Fill="#FF005FAF" HorizontalAlignment="Center" Height="23"  Stroke="#FF0A4670" VerticalAlignment="Center" Width="23.001" RadiusY="12" RadiusX="12" Grid.Column="10" Margin="34.122,16.549,102.716,5.451"/>
        <TextBlock Grid.Column="10" HorizontalAlignment="Center"  TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center" Width="70.038"  Height="15.96" Margin="73.123,19.04,16.678,10"/>
