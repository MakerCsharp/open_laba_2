                <ColumnDefinition Width="170*"/>
                <ColumnDefinition Width="134*"/>
                <ColumnDefinition Width="193*"/>
                <ColumnDefinition Width="42*"/>
            </Grid.ColumnDefinitions>

            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <Grid Grid.Row="0">
                    <TextBlock Text="First Name"/>

                </Grid>
                <Grid Grid.Row="1">
                    <TextBox Width="100" Height="15" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                </Grid>

            </Grid>

            <Grid Grid.Row="1" Grid.Column="2">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <Grid Grid.Row="0">
                    <TextBlock Text="Last Name"/>

                </Grid>
                <Grid Grid.Row="1">
                    <TextBox Width="100" Height="15" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                </Grid>

            </Grid>

            <Grid Grid.Row="1" Grid.Column="3">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <Grid Grid.Row="0">
                    <TextBlock Text="Start date"/>

                </Grid>
                <Grid Grid.Row="1">
                    <DatePicker Width="100" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                </Grid>

            </Grid>

            <Grid Grid.Row="1" Grid.Column="4">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <Grid Grid.Row="0">
                    <TextBlock Text="End date"/>

                </Grid>
                <Grid Grid.Row="1">
                    <DatePicker Width="100" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                </Grid>

            </Grid>

